# Development variant with explicit single NAT gateway profile
aws_region   = "us-east-1"
project_name = "secure-foundation"
environment  = "dev"
owner        = "DevSecOps Team"

vpc_cidr             = "10.40.0.0/16"
az_count             = 2
public_subnet_cidrs  = ["10.40.1.0/24", "10.40.2.0/24"]
private_subnet_cidrs = ["10.40.11.0/24", "10.40.12.0/24"]
allowed_ssh_cidr     = "203.0.113.0/24"

enable_nat_gateway    = true
single_nat_gateway   = true
create_bastion       = false
bastion_instance_type = "t3.micro"
